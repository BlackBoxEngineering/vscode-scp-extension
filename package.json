{
  "name": "scp-spherex",
  "displayName": "SphereScript for SphereServer-X",
  "description": "Syntax highlighting for SphereServer-X script files (scp)",
  "publisher": "sphereserver-uo",
  "version": "1.0.2",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sphereserver/vscode-scp-extension.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "scp",
        "aliases": [
          "SphereScript",
          "SphereScript-X",
          "scp"
        ],
        "extensions": [
          ".scp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "scp",
        "scopeName": "source.scp",
        "path": "./syntaxes/scp.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "oniguruma": "^7.2.0",
    "textmate-bailout": "^1.1.0",
    "textmate-tester": "^1.1.9",
    "@vscode/vsce": "^1.103.1"
  }
}
