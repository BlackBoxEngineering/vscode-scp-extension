{
  "name": "scp-spherex",
  "displayName": "SphereScript for SphereServer-X",
  "description": "Syntax highlighting for SphereServer-X script files (scp)",
  "publisher": "sphereserver-uo",
  "version": "1.0.3",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlackBoxEngineering/vscode-scp-extension.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "scp",
        "aliases": [
          "SCP Script"
        ],
        "extensions": [
          ".scp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "scp",
        "scopeName": "source.scp",
        "path": "./syntaxes/scp.tmLanguage.json"
      }
    ]
  },
  "files": [
    "syntaxes",
    "language-configuration.json",
    "images"
  ],
  "devDependencies": {
    "vscode-tmgrammar-test": "^0.1.1",
    "@vscode/vsce": "^1.103.1"
  }
}
